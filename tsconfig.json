{
    "compilerOptions": {
        "outDir": "./built",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["es2017"],
      "moduleResolution": "node",
      "strict": true,
      "target": "esnext",
      "skipLibCheck": true
    },
    "files": ["App.tsx"],
    "include": ["./src/**/*", "App.tsx"],
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
  }